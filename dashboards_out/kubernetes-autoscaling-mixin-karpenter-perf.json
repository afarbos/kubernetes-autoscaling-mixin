               "exemplar": false,
               "expr": "max(\n  karpenter_nodes_termination_duration_seconds{\n    cluster=\"$cluster\", job=~\"$job\",\n    quantile=\"0.5\"\n  }\n)\n",
               "interval": "1m",
               "legendFormat": "P50"
